<ResourceDictionary  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Calculator" 
        xmlns:v="clr-namespace:System;assembly=mscorlib">

    <v:String x:Key="r_English">English</v:String>
    <v:String x:Key="r_Russian">Русский</v:String>
    <v:String x:Key="r_French">French</v:String>

    <v:String x:Key="multiply">умножить</v:String>
    <v:String x:Key="divide">делить</v:String>
    <v:String x:Key="fold">сложить</v:String>
    <v:String x:Key="subtract">вычесть</v:String>
    <v:String x:Key="history">история</v:String>
    <v:String x:Key="clear">очистить</v:String>
    <v:String x:Key="A">А</v:String>
    <v:String x:Key="B">Б</v:String>
    <v:String x:Key="Result">результат</v:String>
    <v:String x:Key="woof">гаф</v:String>
</ResourceDictionary>
    
    