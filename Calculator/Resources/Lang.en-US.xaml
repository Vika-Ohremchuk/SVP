<ResourceDictionary  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Calculator" 
        xmlns:v="clr-namespace:System;assembly=mscorlib">

    <v:String x:Key="r_English">English</v:String>
    <v:String x:Key="r_Russian">Русский</v:String>
    <v:String x:Key="r_French">French</v:String>

    <v:String x:Key="multiply">multiply</v:String>
    <v:String x:Key="divide">divide</v:String>
    <v:String x:Key="fold">fold</v:String>
    <v:String x:Key="subtract">subtract</v:String>
    <v:String x:Key="history">history</v:String>
    <v:String x:Key="clear">clear</v:String>
    <v:String x:Key="A">A</v:String>
    <v:String x:Key="B">B</v:String>
    <v:String x:Key="Result">Result</v:String>
    <v:String x:Key="woof">woof</v:String>
</ResourceDictionary>
    